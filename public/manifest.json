{
  "manifest_version": 3,
  "name": "Designer",
  "description": "Customize any website",
  "version": "0.0.1",
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "js/vendor.js",
        "js/content_script.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}